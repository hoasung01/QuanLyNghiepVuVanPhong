<div align="left">
Kết quả: <%= @sinhviens.size %> sinh viên
</div>
        <table border ="1" width ="90%" align="center" cellspacing ="0" class="alternative" bgcolor ="white" >
          <tr>
            <th>STT</th>
            <th>Mã SV</th>
            <th>Họ tên</th>
            <th>Ngày sinh</th>
            <th>Giới tính</th>
            <th>Địa chỉ </th>
            <th>Số điện thoại</th>
            <th>Lớp</th>
            <th>Trình độ</th>
            <th>Chọn</th>

          </tr>
          <% dem = 1 %>
          <% @sinhviensearchs.each do |sinhvien| %>
          <tr>
            <th><%= dem %></th>
            <td ><%= sinhvien.MSSV %></td>
            <td><%= sinhvien.HoTenSV %></td>
            <td><%= sinhvien.NgaySinh %></td>
            <td><%= sinhvien.GioiTinh %></td>
            <td><%= sinhvien.DiaChi %></td>
            <td><%= sinhvien.SDT %></td>
            <td><%= sinhvien.lop.TenLop %></td>
            <td><%= sinhvien.trinhdodaotao.TenTDDT %></td>
            <td>
              <input type="checkbox"
                 id="<%= sinhvien.id %>"
                 name="sinhvien_ids[]"
                 class="checksv"
                 data-value="<%= sinhvien.id %>"
                 value="<%= sinhvien.id %>"
                 <% if @baoluu.sinhviens.include? sinhvien%>checked="checked"<%end%>
                 >
            </td>
          </tr>
          <% dem = dem +1 %>
        <% end %>
          
        </table>
<div align="left">
Hiển thị: <%= dem - 1 %> / <%= @sinhviens.size %>
</div>
        <%= will_paginate @sinhviensearchs %>
<br/>
    
      