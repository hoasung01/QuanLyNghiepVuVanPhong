class"BundledAsset"logical_path"application.js"pathname"0$root/app/assets/javascripts/application.js"content_type"application/javascript"
mtime"2012-06-04T22:07:12+07:00"lengthiG	"digest"%da49355c343a4014efff7b653c9d6ef9"source"G	/*!
 * jQuery JavaScript Library v1.7.1
 * http://jquery.com/
 *
